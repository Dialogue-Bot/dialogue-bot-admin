{
  "name": "api",
  "version": "0.0.0",
  "description": "TypeScript + Express API Server",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "concurrently  \"npm run dev:worker\" \"npm run dev:api\" \"npm run typesafe-i18n\"",
    "start": "npm run build  && cross-env NODE_ENV=production concurrently \"npm run start:api\" \"npm run start:worker\" \"npm run typesafe-i18n\"",
    "start:api": "cross-env NODE_ENV=production node dist/server.js",
    "start:worker": "cross-env NODE_ENV=production node dist/worker.js",
    "dev:worker": "nodemon --legacy-watch src/worker.ts --exec \"ts-node -r tsconfig-paths/register --transpile-only src/worker.ts\"",
    "dev:api": "cross-env NODE_ENV=development  nodemon  --legacy-watch ",
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src",
    "lint:fix": "npm run lint -- --fix",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "gen:migrate": "drizzle-kit generate:pg",
    "push:migrate": "drizzle-kit push:pg",
    "drop:migrate": "drizzle-kit drop",
    "check:migrate": "drizzle-kit check:pg",
    "db:studio": "drizzle-kit studio --port 9000",
    "db:seed": "ts-node -r tsconfig-paths/register --transpile-only src/database/seed.ts",
    "typesafe-i18n": "typesafe-i18n",
    "debug": "concurrently cross-env nodemon --exec \"node --inspect=0.0.0.0:9229 --require ts-node/register -r tsconfig-paths/register src/server.ts\" ",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "@react-email/components": "^0.0.15",
    "@react-email/render": "0.0.7",
    "axios": "^1.6.7",
    "bcrypt": "^5.0.1",
    "bullmq": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.1",
    "drizzle-orm": "^0.29.3",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "firebase-admin": "^12.0.0",
    "helmet": "^5.1.1",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "node-nlp": "^4.27.0",
    "nodemailer": "^6.9.8",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.5.0",
    "typedi": "^0.10.0",
    "typesafe-i18n": "^5.26.2",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.220",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.13",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.202",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.11",
    "@types/node": "^17.0.45",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.2.58",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.13",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "node-config": "^0.0.2",
    "node-gyp": "^9.1.0",
    "nodemon": "^2.0.19",
    "pm2": "^5.2.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
